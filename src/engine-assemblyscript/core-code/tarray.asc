type FloatArray = ${FloatArrayType}
type Float = ${FloatType}
type Int = i32


// =========================== EXPORTED API
function x_tarray_createListOfArrays(): FloatArray[] {
    const tarray: FloatArray[] = []
    return tarray
}

function x_tarray_pushToListOfArrays(arrays: FloatArray[], array: FloatArray): void {
    arrays.push(array)
}

function x_tarray_getListOfArraysLength(arrays: FloatArray[]): i32 {
    return arrays.length
}

function x_tarray_getListOfArraysElem(arrays: FloatArray[], index: i32): FloatArray {
    return arrays[index]
}

export {
    x_tarray_createListOfArrays as tarray_createListOfArrays,
    x_tarray_pushToListOfArrays as tarray_pushToListOfArrays,
    x_tarray_getListOfArraysLength as tarray_getListOfArraysLength,
    x_tarray_getListOfArraysElem as tarray_getListOfArraysElem,
    tarray_create,
}


// =========================== TARRAY API
function tarray_create(length: i32): FloatArray {
    return new ${FloatArrayType}(length)
}