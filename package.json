    "homepage": "https://github.com/sebpiq/WebPd_compiler#readme",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^23.0.4",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^10.0.1",
        "@types/jest": "^29.4.0",
        "@types/node": "^14.14.7",
        "@webpd/dev": "file:../WebPd_dev",
        "assemblyscript": "^0.25.1",
        "jest": "^29.4.3",
        "rollup": "^3.7.0",
        "ts-jest": "^29.0.5",
